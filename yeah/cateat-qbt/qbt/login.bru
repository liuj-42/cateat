meta {
  name: login
  type: http
  seq: 2
}

post {
  url: http://192.168.1.173:8080/api/v2/auth/logi
  body: formUrlEncoded
  auth: inherit
}

body:form-urlencoded {
  username: {{username}}
  password: {{password}}
}

vars:post-response {
  cookie: res.headers.Cookie
}

settings {
  encodeUrl: false
  timeout: 0
}
